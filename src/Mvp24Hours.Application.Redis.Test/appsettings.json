{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "Mvp24Hours": {
    "Persistence": {
      "Redis": {
        "Enable": true,
        "DefaultExpiration": null,
        "Hosts": [ "localhost:6379" ],
        "InstanceName": null,
        "DefaultDatabase": 0,
        "AbortOnConnectFail": false,
        "AllowAdmin": true,
        "Ssl": false,
        "ConnectTimeout": 6000,
        "ConnectRetry": 2
      }
    }
  }
}
